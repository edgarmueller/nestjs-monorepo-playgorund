{
  "branches": ["main"],
  "tagFormat": "lib-example-b-v${version}",
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    "@semantic-release/npm",
    ["@semantic-release/git", {
      "assets": ["package.json", "package-lock.json", "CHANGELOG.md"],
      "message": "chore(release): lib-example-b ${nextRelease.version}\n\n${nextRelease.notes}"
    }],
    ["@semantic-release/github", {
       "assets": [
        {"path": "dist/**/*.js", "label": "JS distribution"}
        {"path": "dist/**/*.js.map", "label": "source maps"}
        {"path": "dist/**/*.d.ts", "label": "type defs"}
      ]
    }]
    ],
    "repositoryUrl": "https://github.com/edgarmueller/nestjs-monorepo-playground"
}